.row.wrapper.border-bottom.white-bg.page-heading
  .col-lg-10
    %h2 Products list
    %ol.breadcrumb
      %li
        %a{:href => ""} Home
      %li
        %a E-commerce
      %li.active
        %strong Products list
  .col-lg-2
.wrapper.wrapper-content.animated.fadeInRight.ecommerce
  .ibox-content.m-b-sm.border-bottom
    .row
      .col-sm-4
        .form-group
          %label.control-label{:for => "product_name"} Product Name
          %input#product_name.form-control{:name => "product_name", :placeholder => "Product Name", :type => "text", :value => ""}
      .col-sm-2
        .form-group
          %label.control-label{:for => "price"} Price
          %input#price.form-control{:name => "price", :placeholder => "Price", :type => "text", :value => ""}
      .col-sm-2
        .form-group
          %label.control-label{:for => "quantity"} Quantity
          %input#quantity.form-control{:name => "quantity", :placeholder => "Quantity", :type => "text", :value => ""}
      .col-sm-4
        .form-group
          %label.control-label{:for => "status"} Status
          %select#status.form-control{:name => "status"}
            %option{:selected => "", :value => "1"} Enabled
            %option{:value => "0"} Disabled
  .row
    .col-lg-12
      .ibox
        .ibox-content
          %table.footable.table.table-stripped.toggle-arrow-tiny{"data-page-size" => "15"}
